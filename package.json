{
  "name": "spacewars",
  "version": "1.0.0",
  "description": "A space wars html game",
  "engines": {
    "node": ">= 8.11.3",
    "npm": "~5.6.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base src/ --mode development --open --config webpack.dev.js",
    "dev": "webpack --mode development --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.prod.js",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakioman/spacewars.git"
  },
  "keywords": [
    "html",
    "space",
    "wars",
    "pixijs",
    "pixi",
    "javascript"
  ],
  "author": "Nacho Glinsek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nakioman/spacewars/issues"
  },
  "homepage": "https://github.com/nakioman/spacewars#readme",
  "dependencies": {
    "howler": "^2.1.1",
    "pixi-particles": "^3.1.0",
    "pixi.js": "^4.8.4"
  },
  "devDependencies": {
    "@types/howler": "^2.0.5",
    "@types/pixi.js": "^4.8.6",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "terser-webpack-plugin": "^1.2.1",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "tslint-webpack-plugin": "^2.0.1",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  }
}
